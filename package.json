{
  "name": "penshare",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=unitTest jest",
    "pretest": "cross-env NODE_ENV=unitTest npm run migrate:reset",
    "start": "node server",
    "dev": "nodemon server",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "build": "cd react-ui/ && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodrigofazenda/penshare.git"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_nodules/",
      "<rootDir>/react-ui"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rodrigofazenda/penshare/issues"
  },
  "homepage": "https://github.com/rodrigofazenda/penshare#readme",
  "dependencies": {
    "@types/lodash.throttle": "^4.1.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-auth-library": "^7.0.3",
    "mysql2": "^2.2.5",
    "nanoid": "^3.1.22",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.6.2"
  },
